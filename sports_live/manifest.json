{
  "name": "blast post",
  "description": "blast post for us.",
  "version": "1.0.0",
  "manifest_version": 2,
  "web_accessible_resources": ["*"],
  "permissions": ["storage", "background", "<all_urls>"],
  "browser_action" : {
    "default_icon": "images/get_started16.png",
    "default_title": "popup",
    "default_popup": "html/popup.html"
  },
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css": ["css/style.css"],
      "js": ["js/jquery-3.4.0.min.js", "js/jquery.pause.min.js", "js/script.js"]
    }
  ]
}
